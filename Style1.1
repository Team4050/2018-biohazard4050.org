body {
    margin: 0;
    padding: 0;
    font-family: 'Rubik', sans-serif;
}

.content {
    padding-left: 2%;
    padding-top: 0.5; 
}

.sitemenu {
    width: 100%;
    background: #1D9B48;
    border-top: 10px solid #58595B;
    padding-bottom: 3%;
}

.sitemenu>.navwrapper {
    display: table;
    margin: 0 auto;
}

.sitemenu ul {
    padding: 0;
    margin 0;
    list-style-type: none;
}

.sitemenu ul li {
    float: left;
}

.sitemenu ul li a {
    float: left;
    text-decoration: none;
    padding: 0 15px;
    line-height: 50px;
    color: black;
}

.sitemenu ul li a:hover {
    background-color: #45B29D;
}



.sitemenu ul li.active a {
    color: black;
}

html {
    font-size: 16px;
}

header {
    width: 100%;
    padding: 0;
    font-size: 50px;
}

img {
    display: block;
    margin: 0 auto;
    max-width: 75%;
    align-content: center;
}

#header {
    text-align: center;
}

h1 {
    text-align: center;
}

p {
    padding-left: 5%;
}

#team-values {
    text-align: center;
}

#description {
    text-align: center;
}
* {
    box-sizing: border-box;
}
.column {
    float: left;
    width: 25%;
    padding: 20px;
    height: 300px;
}
.row:after {
    content: "";
    display: table;
    clear: both;
}
